{
  "name": "voiceping-router",
  "version": "1.0.0",
  "description": "Reliable, secure real-time audio communication for coordinating 1000+ distributed team members during high-profile events",
  "main": "./dist/server/index.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/server/index.ts",
    "build": "tsc && npm run build:client",
    "build:client": "esbuild src/client/test/pttDemo.ts --bundle --outfile=dist/client/test/pttDemo.js --format=esm --platform=browser --target=es2020",
    "start": "node dist/server/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:load": "npx tsx src/server/test/loadTest.ts",
    "lint": "eslint src/shared src/server --ext .ts"
  },
  "keywords": [
    "PTT",
    "push to talk",
    "walkie talkie",
    "voiceping",
    "smartwalkie",
    "smartwalkietalkie",
    "library",
    "SDK"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SmartWalkieOrg/voiceping-router"
  },
  "author": "Smart Walkie",
  "contributors": [
    {
      "name": "Muhammad Muthi Dhian Pratama",
      "email": "dhian@smartwalkietalkie.com"
    },
    {
      "name": "Iqbal N",
      "email": "iqbal@smartwalkietalkie.com"
    },
    {
      "name": "Sirius Dely",
      "email": "sirius@smartwalkietalkie.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.0",
    "jsonwebtoken": "^9.0.2",
    "mediasoup": "^3.19.16",
    "mediasoup-client": "^3.18.6",
    "rate-limiter-flexible": "^9.1.0",
    "redis": "^4.6.0",
    "winston": "^3.11.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "esbuild": "^0.19.12",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0",
    "vitest": "^1.3.0"
  }
}
